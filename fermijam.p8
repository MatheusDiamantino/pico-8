function init_obj(type, x, y, rotation, _message)
	local obj={}
	obj.type = type
	obj.x = x
	obj.y = y
	obj.hitbox={x=0, y=0, w=8, h=8}
	obj.rotation=rotation

	if obj.type==message then obj.text=_message end

	-- collision between actors
	obj.overlap_actor=function(ox, oy)
		for other in all(objects) do
			if other~=nil and other!=obj and
				other.x+other.hitbox.x+other.hitbox.w > obj.x+obj.hitbox.x+ox and
				other.y+other.hitbox.y+other.hitbox.h > obj.y+obj.hitbox.y+oy and
				other.x+other.hitbox.x < obj.x+obj.hitbox.x+obj.hitbox.w+ox and
				other.y+other.hitbox.y < obj.y+obj.hitbox.y+obj.hitbox.h+oy then

				return other
			end
		end
		return nil
	end

	obj.type.init(obj)
	add(objects, obj)

	return obj
end

function clear_obj()
	foreach(objects, function(obj)
		if obj.type!=ship and obj.type!=fire then del(objects, obj) end
	end)
	system_transmition=0
end

function generate(x, y, _space, planets, stars, _has_sun)
	local n_star=stars
	local n_planets=planets
	local space=_space
	local has_sun=_has_sun

	if has_sun then
		_sun=init_obj(sun, x, y)
		p_distance=0
	end

	for i=0, n_star do
		init_obj(star, rnd(space)-space/2+x, rnd(space)-space/2+y)
	end

	for i=0, n_planets do
		p_distance+=50+rnd(100)
		init_obj(planet, rnd(space)-space/2+x, rnd(space)-space/2+y)
	end

end

function generate_info()

	local name = "* planet 0x"
	local life = "life: " .. _life[flr(rnd(n_life))+1]
	local weather = "weather: " .. _weather[flr(rnd(n_weather))+1]

	for i=1, 8 do
		if rnd(1) < 0.5 then 
			local odds = rnd(6)
			if odds > 0 and odds <= 1 then
				name = name .. "a"
			elseif odds >= 1 and odds <= 2 then
				name = name .. "b"
			elseif odds > 2 and odds <= 3 then
				name = name .. "c"
			elseif odds > 3 and odds <= 4 then
				name = name .. "d"
			elseif odds > 4 and odds <= 5 then
				name = name .. "e"
			elseif odds > 5 and odds <= 6 then
				name = name .. "f"
			end
		else name = name .. flr(rnd(10)) end
	end

	name = name .. " *"

	return name .. "#" .. life .. "#" .. weather

end

function _draw()
	cls()

	if shake>0 then
		shake-=1
		camera(player.x-64+rnd(2)-1, player.y-64+rnd(2)-1)
	else	
		camera(player.x-64, player.y-64)
	end

	draw_message=nil
	foreach(objects, function(obj)
				if obj.type~=message then obj.type.draw(obj) else draw_message=obj end
		end)

	if player~=nil then player.type.draw(player) end

	camera()
	print(stat(0) .. "  " .. stat(1), 50, 0)
	hud.draw(this)
	
	if draw_message~=nil then draw_message.type.draw(draw_message) end
	--print(message)


end

function _update()

	foreach(objects, function(obj)
				obj.type.update(obj)
		end)

	hud.update(hud)

end

function _init()
	generate(64, 64, 1024, 10, 500, true)
	hud.init(hud)
	player=init_obj(ship, 64, 64)

end

--[[
	// quick and dirty way of rotating a sprite
	sx = spritecheet x-coord
	sy = spritecheet y-coord
	sw = pixel width of source sprite
	sh = pixel height of source sprite
	px = x-coord of where to draw rotated sprite on screen
	py = x-coord of where to draw rotated sprite on screen
	r = amount to rotate (radians)
	s = 1.0 for normal scale, 0.5 for half, etc
]]
function draw_rotated(sx,sy,sw,sh,px,py,r,s)
	-- loop through all the pixels
	for y=sy,sy+sh,1 do
		for x=sx,sx+sw,1 do
			-- get source pixel color
			col = sget(x,y)
			-- skip transparent pixel (zero in this case)
			if (col != 0) then
				-- rotate pixel around center
				local xx = (x-sx)-sw/2
				local yy = (y-sy)-sh/2
				local x2 = (xx*cos(r) - yy*sin(r))*s
				local y2 = (yy*cos(r) + xx*sin(r))*s
				-- translate rotated pixel to where we want to draw it on screen
				local x3 = flr(x2+px)
				local y3 = flr(y2+py)
				-- use rectfill if scale is > 1, otherwise just pixel it
				if (s >= 1) then
					local w = flr(x2+px+s)
					local h = flr(y2+py+s)
					rectfill(x3,y3,w,h,col)
				else
					pset(x3,y3,col)
				end
			end
		end
	end
end

-- helper functions --
----------------------

function clamp(val,a,b)
	if b>a then 
		return max(a, min(b, val))
	else
		return max(b, min(a, val))
	end
end

function appr(val, target, amount)
	return val > target and max(val-amount, target) or min(val+amount, target)
end

function sign(val)
	return (val>0 and 1) or (val<0 and -1) or 0
end


