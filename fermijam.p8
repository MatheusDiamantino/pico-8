n_life=3
_life={"void", "intermitent", "abundant"}

n_message=0
_messages={}

p_distance=0
_sun=nil

ship={
	
	init=function(this)
		this.rotation=0
		this.target_rotation=0
		this.scale=1
		this.speed=0
		this.dir=0
		this.p_shoot=false
		this.ship_state=0
		this.docking_state=0
		this.hd_state=0
		this.planet_dock=nil
		this.target={x=0, y=0}
		this.message=nil
		this.p_hyperdrive_release = false
		this.hd_timer=0
	end,

	update=function(this)
		local hyperdrive_press = btn(k_hyperdrive, 1)

		local hyperdrive_release = not btn(k_hyperdrive, 1) and this.p_hyperdrive_release
		this.p_hyperdrive_release = btn(k_hyperdrive, 1)


		----------------------------------------
		-- 			normal state              --
		----------------------------------------
		if this.ship_state==0 then

			local input = btn(k_up) and 1 or (btn(k_down) and 0 or 0)
			local _break = btn(k_down)
			local shoot = btn(k_shoot) and not this.p_shoot
			this.p_shoot = btn(k_shoot)

			if btn(k_up) then this.dir=1 elseif btn(k_down) then this.dir=-1 end

			-- planet collision
			this.planet_dock=this.overlap_actor(0, 0)
			if this.planet_dock~=nil and this.planet_dock.type==planet and this.ship_state==0 then 
				this.ship_state=1
				this.message=init_obj(message, this.x-56, this.y+32, nil, "-- landing... --")
				this.target.x=this.planet_dock.x
				this.target.y=this.planet_dock.y
			end
			

			-- rotate ship
			if btn(k_right) then this.target_rotation-=0.05
			elseif btn(k_left) then this.target_rotation +=0.05 end

			this.rotation=appr(this.rotation, this.target_rotation, 0.03)

			-- movement
			local target_speed=2
			local friction=0.05

			if input == 0 then target_speed=0 friction = 0.01 end

			if _break then target_speed=0 friction=0.08 end

			if btn(k_pulsedrive) and drive_fuel>0 then 
				target_speed=5 
				friction=0.1 
				drive_fuel-=1 
			elseif drive_fuel<100 and not btn(k_pulsedrive) then
				drive_fuel+=1 
			end

			this.speed=appr(this.speed, target_speed, friction)
			-- this.x+=this.speed*sin(this.rotation) 
			-- this.y-=this.speed*cos(this.rotation)

			
		----------------------------------------
		-- 			docking state             --
		----------------------------------------
		elseif this.ship_state==1 then
			
			

			-- ajusting position
			if this.docking_state==0 then
				this.target.x=this.planet_dock.x
				this.target.y=this.planet_dock.y

				this.speed=0
				this.x=appr(this.x, this.target.x, 1.5)
				this.y=appr(this.y, this.target.y, 1.5)
				this.rotation=appr(this.rotation, flr(this.rotation+0.5), 0.02)

				-- changing state
				if this.rotation==flr(this.rotation) and this.x==this.target.x and this.y==this.target.y then 
					this.docking_state=1 
				end

			-- docking
			elseif this.docking_state==1 then
				this.scale = appr(this.scale, 0.2, 0.02)
				this.x=this.planet_dock.x
				this.y=this.planet_dock.y

				-- changing state
				if this.scale==0.2 then 
					this.docking_state=2 
					del(objects, this.message)
					
					if this.planet_dock~=nil and this.planet_dock.transmition then
						this.message=init_obj(message, this.x-56, this.y+25, nil, "* transmition incoming *#'@($*&ˆ%@*$%&#!@*&!%ˆ&@$!@#!ˆ@&$ˆ*&!@ˆ$&@'")

					elseif this.planet_dock~=nil then
						this.message=init_obj(message, this.x-56, this.y+25, nil, this.planet_dock.info)
					end
				end

			-- getting message state
			elseif this.docking_state==2 then
				this.x=this.planet_dock.x
				this.y=this.planet_dock.y

				if btn(k_up) then 
					this.docking_state=3
				end

			-- getting off the planet
			elseif this.docking_state==3 then

				this.scale = appr(this.scale, 1, 0.01)
				this.speed=appr(this.speed, 2, 0.01)
				this.x+=this.speed*sin(this.rotation) 
				this.y-=this.speed*cos(this.rotation)

				-- end of docking process
				if this.scale==1 then this.ship_state=0 this.docking_state=0 del(objects, this.message) end
			end


		----------------------------------------
		-- 			hyperdrive state          --
		----------------------------------------
		elseif this.ship_state==2 then
			shake=1
			if this.hd_state==0 then
				this.hd_timer+=1

				this.speed=appr(this.speed, 2, 0.1)

				if this.speed==2 and this.hd_timer>20 then this.hd_state=1 this.hd_timer=0 end
				if hyperdrive_release then this.ship_state=0 this.hd_state=0 this.hd_timer=0 end

			elseif this.hd_state==1 then

				this.speed=8

				clear_obj() 
				generate(this.x, this.y, 128, -1, 15)

				if hyperdrive_release then this.hd_state=2 this.speed=20 generate(this.x, this.y, 1024, 5, 500, true) music(-1) music(3) end

			elseif this.hd_state==2 then

				this.speed=appr(this.speed, 1, 1.5)

				if this.speed==1 then this.hd_state=0 this.ship_state=0 end

			end

		end

		-- hyperdrive generation code
		if this.ship_state!=1 then
			if this.ship_state!=2 and hyperdrive_press then this.ship_state=2 music(0)
			elseif hyperdrive_release and this.hd_state==0 then  music(-1) end
		end

		this.x+=this.speed*sin(this.rotation) 
		this.y-=this.speed*cos(this.rotation)
	end,

	draw=function(this)
		draw_rotated(8, 0, 8, 8, this.x+4, this.y+4, this.rotation, this.scale)

		if rnd(1) < 0.4 then for i=0.5, this.speed do init_obj(fire, this.x+4-3*sin(this.rotation-rnd(0.05)+0.05) , this.y+4+3*cos(this.rotation-rnd(0.05)+0.05) , this.rotation) end end
		
		--rectfill(this.x, this.y, this.x+this.hitbox.x+this.hitbox.w, this.y+this.hitbox.y+this.hitbox.h, white)
	end
}

planet={
	init=function(this)
		this.timer=0
		this.spawn_time = 200
		this.radius = 10
		this.col=flr(rnd(15)+1)
		this.docked=false
		this.transmition=false
		if system_transmition < max_system_transmition then this.transmition=true system_transmition+=1 end
		this.info=generate_info()
		this.t=rnd(1)
		this.distance=p_distance
		this.center={x=0, y=0}

	end,

	update=function(this)
		if this.transmition and this.timer > this.spawn_time+rnd(10) then
			init_obj(transmition, this.x+4, this.y+4)
			this.timer=0
		end

		this.x=this.distance*cos(this.t) + _sun.x
		this.y=2*this.distance*sin(this.t) + _sun.y
		this.t+=0.0001

		this.timer+=1

	end,

	draw=function(this)
		circfill(this.x+4, this.y+4, this.radius, this.col)
		--rectfill(this.x, this.y, this.x+this.hitbox.x+this.hitbox.w, this.y+this.hitbox.y+this.hitbox.h, white)
	end,

}

