pico-8 cartridge // http://www.pico-8.com
version 7
__lua__
--paint demo

poke(0x5f2d, 1)
b = {}							--input variable
paint = {}			--the paint on the canvas
brush = {}			--brush the player controls
brush.x = 64
brush.y = 64
brush.r = -1
brush.color = 2
brush.painting=false
t = {}
t = 0
canvascol = {}
canvascol = 13
menu = {}
menu = true

function get_input()
	b[0] = btn(0)
	b[1] = btn(1)
	b[2] = btn(2)
	b[3] = btn(3)
	b[4] = stat(34)
	b[5] = btnp(5)
	b[6] = btnp(4, 1)
	b[7] = btnp(5, 1)
end

function handle_input()
	--if b[0] then brush.x -= 1 end
	--if b[1] then brush.x += 1 end
	--if b[2] then brush.y -= 1 end
	--if b[3] then brush.y += 1 end
	brush.x = stat(32)
	brush.y = stat(33)
	if b[4] == 1 then
	
		menu = false
		brush.painting = true
	else
		brush.painting = false
	end
	
	if b[5] then
		brush.color += 1
		if brush.color > 15 then
			brush.color = 0
		end
	end
	
	if b[6] then
		canvascol += 1
		if canvascol > 15 then
			canvascol = 0
		end
	end
	
	if b[7] then
		clear_canvas()
	end
end

function clear_canvas()
	for v in all(paint) do
		del(paint, v)
	end
end

function make_paint()
	local tmax = 1
	if brush.painting then
		brush.r = 3
	else
		if t >= tmax then
			brush.r -= 1
			t = 0
		else
			t += 1
		end
		if brush.r < 0 then
			brush.r = -1
		end
	end
	
	if brush.r >=  0 then
		local p = create_paint(brush.x,
														brush.y,
														brush.r,
														brush.color)
		add(paint,p)
	end
end

function create_paint(x, y, r, col)
	local p = {}
	p.x = x
	p.y = y
	p.r = r
	p.col = col
	return p
end

function draw_hud()
	
	if menu then
	print(stat(34), 0, 0, 7)
	print("press z to start", 35, 55, 7)
	print("shift - canvas color", 2, 110, 7)
	print("a - clear canvas", 2, 100, 7)
	print("x - change color, z - paint", 2, 120, 7)
	else
		print("color", 2, 2, 7)
		circfill(30, 4, 2, brush.color)
	end
end

function draw_background()
	rectfill(0, 0, 127, 127, canvascol)
end

function draw_brush()
	if brush.painting then
		spr(3, brush.x, brush.y-24, 1, 3)
		spr(2, brush.x, brush.y)
	else
		spr(3, brush.x, brush.y-24, 1, 3)
		spr(1, brush.x, brush.y)
	end
end

function draw_paint()
	for v in all(paint) do
		circfill(v.x+4, v.y+8, v.r, v.col)
	end
end

function add_paint(p)
	for v in all(paint) do
		if(v.x == p.x and v.y == p.y and v.r>p.r and v.col == p.col) then
			return
		end
	end
	add(paint, p)
	return
end

function _init()
	cls()
end

function _update()
	get_input()
	handle_input()
	if menu == false then
		make_paint()
	end
end

function _draw()
	cls()
	draw_background()
	draw_hud()
	if menu then
	else
	draw_paint()
	draw_brush()
	end
end

